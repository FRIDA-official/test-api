{
  "openapi": "3.0.1",
  "info": {
    "title": "FRIDA PensionSerivce",
    "description": "This is a proof of Concept API for demonstration purposes.  You can find out more about     FRIDA at [https://www.freeinsurancedata.de/](https://www.freeinsurancedata.de/) or on [https://github.com/FRIDA-official](https://github.com/FRIDA-official).      For this sample, you can use the api key `special-key` to test the authorization     filters.",
    "termsOfService": "https://github.com/FRIDA-official",
    "contact": {
      "email": "anton.rott@accenture.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "0.0.1"
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  },
  "servers": [
    {
      "url": "http://test-api-impl-spring.herokuapp.com/v2/"
    }
  ],
  "tags": [
    {
      "name": "Contract",
      "description": "Everything about Contracts",
      "externalDocs": {
        "description": "Find out more",
        "url": "https://www.freeinsurancedata.de/"
      }
    }
  ],
  "paths": {
    "/Contract": {
      "get": {
        "tags": [
          "Contract"
        ],
        "summary": "Get all Contracts",
        "description": "This get service returns all contracts available",
        "operationId": "getAllContracts",
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Contract"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid status value",
            "content": {}
          }
        }
      }
    },
    "/Contract/{contractId}": {
      "get": {
        "tags": [
          "Contract"
        ],
        "summary": "Find Contract by ID",
        "description": "Returns a single Contract",
        "operationId": "getContractById",
        "parameters": [
          {
            "name": "contractId",
            "in": "path",
            "description": "ID of Contract to return",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Contract"
                }
              }
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "Contract not found"
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "Date": {
        "type": "string",
        "format": "date",
        "description": "Date in YYYY-DD-MM Format",
        "example": "1992-02-12",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "Contract": {
        "type": "object",
        "properties": {
          "internaContractlUUID": {
            "type": "string",
            "example": "7785d3ea-6a52-45c0-98ac-246c31608b48"
          },
          "typeOfInsurance": {
            "type": "string",
            "example": "Pension"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "description": "Timestamp of Request",
            "example": "2018-03-20T09:12:28Z"
          },
          "beginOfInsurance": {
            "$ref": "#/components/schemas/Date"
          },
          "beginOfPension": {
            "$ref": "#/components/schemas/Date"
          },
          "currentPensionPayments": {
            "type": "number",
            "example": "2500"
          },
          "currentPensionAssets": {
            "type": "number",
            "example": "40000"
          },
          "guaranteedInterest": {
            "type": "number",
            "example": "5"
          },
          "currentProfitSharing": {
            "type": "number",
            "example": "123"
          },
          "currentTotalInterest": {
            "type": "number",
            "example": "400000"
          },
          "currentPensionSum": {
            "type": "number",
            "example": "1000000"
          },
          "maximumPensionSum": {
            "type": "number",
            "example": "2000000"
          },
          "maximumPensionSumProfitSharing": {
            "type": "number",
            "example": "40000"
          },
          "currentFundCapital": {
            "type": "number",
            "example": "123123123"
          },
          "maximumFundCapital": {
            "type": "number",
            "example": "312000"
          }
        }
      }
    },
    "securitySchemes": {
      "petstore_auth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://petstore.swagger.io/oauth/dialog",
            "scopes": {
              "write:pets": "modify pets in your account",
              "read:pets": "read your pets"
            }
          }
        }
      },
      "api_key": {
        "type": "apiKey",
        "name": "api_key",
        "in": "header"
      }
    }
  }
}